{
  "name": "node-challenge",
  "version": "1.0.0",
  "description": "Node challeng made to Compasso recruitment process.",
  "main": "server.js",
  "repository": "git@github.com:LeoF99/node-challenge.git",
  "scripts": {
    "test": "jest --config ./jest/jest.config.js",
    "dev": "ts-node-dev src/server.ts",
    "start": "ts-node dist/src/server.js",
    "build": "yarn run clean; tsc -p .;yarn run copy-essentials",
    "clean": "rm -rf dist",
    "copy-essentials": "mkdir -p dist/src/contracts && cp src/contracts/*.yaml dist/src/contracts",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "prom-client": "^13.1.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.33",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@stryker-mutator/core": "^4.6.0",
    "@stryker-mutator/jest-runner": "^4.6.0",
    "@stryker-mutator/typescript-checker": "^4.6.0",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.29",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-boundaries": "^2.1.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/domain/**/*.(t|j)s"
    ]
  }
}
